<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RoundedSquare">
        <item name="cornerSizeBottomLeft">70%</item>
        <item name="cornerSizeBottomRight">70%</item>
    </style>
    <style name="RoundPhoto">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">rounded</item>
    </style>
    <style name="actionBtn" parent="@android:style/Widget.Button">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:shadowColor">#FF000000</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">-1</item>
        <item name="android:shadowRadius">0.2</item>
        <item name="android:textSize">16dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/btn_shape</item>
        <item name="backgroundColor">@color/material_dynamic_primary50</item>
    </style>


</resources>